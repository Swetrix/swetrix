{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "remix dev",
    "deploy": "flyctl deploy --remote-only",
    "import-env": "node fly_import_env.js --env-file .env",
    "dev:staging": "cross-env STAGING=true remix dev",
    "start": "remix-serve build",
    "start:staging": "cross-env STAGING=true remix-serve build",
    "test": "jest --transformIgnorePatterns \"node_modules/(?!axios)/\"",
    "test:ci": "CI=true npm run test",
    "typecheck": "tsc",
    "lint": "eslint app",
    "lint:fix": "eslint app --fix",
    "lint:tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@blaumaus/react-alert": "^7.0.5",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@nivo/core": "^0.83.0",
    "@nivo/sankey": "^0.83.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@remix-run/node": "^1.19.3",
    "@remix-run/react": "^1.19.3",
    "@remix-run/serve": "^1.19.3",
    "@remix-run/v1-route-convention": "^0.1.2",
    "@swetrix/sdk": "^1.1.0",
    "@tailwindcss/forms": "^0.5.6",
    "@testing-library/jest-dom": "^6.1.3",
    "axios": "^1.5.0",
    "axios-auth-refresh": "^3.3.6",
    "babel-jest": "^29.6.4",
    "billboard.js": "^3.9.4",
    "clsx": "^2.0.0",
    "d3": "^7.8.5",
    "dangerously-set-html-content": "^1.0.13",
    "dayjs": "^1.11.9",
    "debug": "^4.3.4",
    "dom-to-image": "^2.6.0",
    "file-saver": "^2.0.5",
    "flatpickr": "^4.6.13",
    "i18n-iso-countries": "^7.6.0",
    "i18next": "^23.4.9",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-fs-backend": "^2.1.5",
    "i18next-http-backend": "^2.2.2",
    "isbot": "^3.6.13",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "marked": "^9.1.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flagkit": "^2.0.4",
    "react-flatpickr": "^3.10.13",
    "react-i18next": "^13.2.2",
    "react-outside-click-handler": "^1.3.0",
    "react-qr-code": "^2.0.12",
    "react-redux": "^8.1.2",
    "react-select": "^5.7.4",
    "react-timezone-select": "^2.1.2",
    "react-type-animation": "^3.1.0",
    "redux": "^4.2.1",
    "redux-saga": "^1.2.3",
    "remix-i18next": "^5.3.0",
    "remix-sitemap": "^2.2.7",
    "remix-utils": "^6.6.0",
    "spacetime": "^7.4.7",
    "swetrix": "^2.4.0",
    "timezone-soft": "^1.4.1",
    "ts-jest": "^29.1.1"
  },
  "eslintConfig": {
    "extends": [
      "@remix-run/eslint-config",
      "@remix-run/eslint-config/jest-testing-library",
      "eslint:recommended"
    ],
    "globals": {
      "JSX": "readonly"
    },
    "plugins": [
      "lodash",
      "react-hooks"
    ],
    "rules": {
      "testing-library/prefer-user-event": "off",
      "jsx-quotes": [
        "error",
        "prefer-single"
      ],
      "lodash/prefer-lodash-method": "warn",
      "comma-dangle": ["warn", "always-multiline"],
      "@typescript-eslint/consistent-type-imports": "off",
      "jsx-a11y/anchor-has-content": "off",
      "no-unused-vars": "off",
      "react-hooks/rules-of-hooks": "error",
      "semi": [
        "error",
        "never"
      ]
    },
    "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": [
            "node_modules",
            "app"
          ],
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      }
    },
    "env": {
      "browser": true,
      "jest": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@remix-run/dev": "^1.19.3",
    "@remix-run/eslint-config": "^1.19.3",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/react": "^14.0.0",
    "@types/debug": "^4.1.8",
    "@types/jest": "^29.5.4",
    "@types/lodash": "^4.14.198",
    "@types/node": "^20.5.9",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-flatpickr": "^3.8.8",
    "@types/react-test-renderer": "^18.0.1",
    "@types/redux-mock-store": "^1.0.3",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/parser": "^6.6.0",
    "@typescript-eslint/typescript-estree": "^6.6.0",
    "autoprefixer": "^10.4.15",
    "cross-env": "^7.0.3",
    "eslint": "^8.48.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "postcss": "^8.4.29",
    "resize-observer-polyfill": "^1.5.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16"
  }
}
